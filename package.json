{
	"name": "its_client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"start": "cross-env BROWSER=none craco start",
		"build": "craco build",
		"test": "craco test"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"airbnb-base",
			"prettier"
		],
		"rules": {
			"@typescript-eslint/no-use-before-define": [
				"error"
			],
			"import/prefer-default-export": 0,
			"import/no-unresolved": 0,
			"no-use-before-define": 0,
			"no-nested-ternary": 0,
			"jsx-a11y/alt-text": 0,
			"global-require": 0
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint --fix",
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@craco/craco": "^6.4.3",
		"@emotion/css": "^11.9.0",
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.0.1",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.1",
		"@types/node": "^16.11.26",
		"@types/react": "^18.0.3",
		"@types/react-dom": "^18.0.0",
		"axios": "^0.26.1",
		"craco-alias": "^3.0.1",
		"cross-env": "^7.0.3",
		"emotion-reset": "^3.0.1",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-router-dom": "^6.3.0",
		"react-scripts": "5.0.1",
		"typescript": "^4.6.3",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier": "^2.6.2"
	}
}
