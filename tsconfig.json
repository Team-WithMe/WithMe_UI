{
  "compilerOptions": {
    "outDir": "build",
    "module": "esnext",
    "lib": ["DOM", "ESNext"],
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "moduleResolution": "node"
  }
}
